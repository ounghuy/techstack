  "scripts": {
    "start": "craco start",
    "start:development": "craco start",
    "start:staging": "env-cmd -f .env.sit craco start",
    "start:uat": "env-cmd -f .env.uat craco start",
    "start:uat2": "env-cmd -f .env.uat2 craco start",
    "build": "craco build",
    "build:development": "env-cmd -f .env.development craco build",
    "build:staging": "env-cmd -f .env.staging craco build",
    "build:uat": "env-cmd -f .env.uat craco build",
    "build:uat2": "env-cmd -f .env.uat2 craco build",
    "build:sit": "env-cmd -f .env.sit craco build",
    "build:production": "env-cmd -f .env.production craco build",
    "build:fullprod": "env-cmd -f .env.fullprod craco build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "format": "pretty-quick --staged --pattern '**/*.{ts,tsx,js}'",
    "lint": "eslint --fix src/**/*.{ts,tsx} --no-ignore",
    "generate": "bash gen-networking.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "react-error-overlay": "6.0.11"
  },
  "optionalDependencies": {
    "babel-loader": "8.1.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@openapitools/openapi-generator-cli": "^2.1.26",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/diacritics": "^1.3.1",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-redux": "^7.1.16",
    "@types/react-table": "^7.7.1",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-saga": "^0.10.5",
    "@types/redux-saga-tester": "^1.0.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "axios-mock-adapter": "^1.19.0",
    "buffer": "npm:buffer@6.0.3",
    "craco-alias": "^2.1.1",
    "craco-plugin-scoped-css": "^1.1.0",
    "crypto": "npm:crypto-browserify@3.12.0",
    "eslint": "^7.18.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-error-overlay": "6.0.9",
    "redux-devtools": "^3.7.0",
    "redux-mock-store": "^1.5.4",
    "snapshot-diff": "^0.8.1",
    "stream": "npm:stream-browserify@3.0.0",
    "util": "npm:util@0.12.5",
    "msw": "^2.1.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && yarn tsc"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@bo(.*)$": "<rootDir>/src$1",
      "^@shared-components(.*)$": "<rootDir>/src/components$1",
      "^@app-pages(.*)$": "<rootDir>/src/pages$1",
      "^@assets(.*)$": "<rootDir>/src/assets$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1",
      "^@strings(.*)$": "<rootDir>/src/utils/strings",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@redux/(.*)$": "<rootDir>/src/redux/$1"
    },
    "coverageReporters": [
      "text",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.node/",
      "/jest/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/index.tsx",
      "!src/**/*/index.{ts,js,tsx}",
      "!src/reportWebVitals.ts",
      "!src/react-app-env.d.ts",
      "!src/networking/**/*",
      "!src/redux/root-saga.ts",
      "!src/redux/store.ts",
      "!src/redux/rootReducer.ts"
    ]
  }
}
